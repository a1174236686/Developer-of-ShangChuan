.rateCtn{
  display: flex;
}

.rateCtn .rateItem{
  position: relative;
}

.rateCtn .rateItem+.rateItem{
  margin-left: 8rpx;
}

.rateCtn .rateItem .starsOff, .starsOn{
  display: block;
  position: absolute;
  font-family: "iconfont" !important;
  font-size: 27rpx;
  font-style: normal;
  color:#FFCE0C;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.rateCtn .rateItem .starsOff:before, .starsOn:before{
  content: "\e611";
}


.rateCtn .rateItem 
.width1,
.width2,
.width3,
.width4,
.width5,
.width6,
.width7,
.width8,
.width9
{
  overflow: hidden;
  Z-index:999;
}

.rateCtn .rateItem .width1{
  width: 10%;
}

.rateCtn .rateItem .width2{
  width: 20%;
}

.rateCtn .rateItem .width3{
  width: 30%;
}

.rateCtn .rateItem .width4{
  width: 40%;
}

.rateCtn .rateItem .width5{
  width: 50%;
}

.rateCtn .rateItem .width6{
  width: 60%;
}

.rateCtn .rateItem .width7{
  width: 70%;
}

.rateCtn .rateItem .width8{
  width: 80%;
}

.rateCtn .rateItem .width9{
  width: 90%;
}
