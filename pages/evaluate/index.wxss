/* pages/evaluate/index.wxss */
.container{
  padding: 30rpx;
}

.container .PhotographerInfoWrap{
  display:flex;
  margin-top: 13rpx;
  padding-bottom: 56rpx;
  border-bottom: 1rpx solid rgba(231,231,231,1);
}

.container .PhotographerInfoWrap .profilePictureWrap image{
  width: 140rpx;
  height: 140rpx;
  border-radius: 70rpx;
}

.container .PhotographerInfoWrap .infoContent{
  margin-left: 40rpx;
  margin-top: 16rpx;
}

.container .PhotographerInfoWrap .infoContent .name{
  display: flex;
  align-items: center;
}

.container .PhotographerInfoWrap .infoContent .name text{
  font-size:32rpx;
  font-weight:bold;
  color:rgba(51,51,51,1);
}

.container .PhotographerInfoWrap .infoContent .name .levelTlele{
  margin-left: 16rpx;
  width: 135rpx;
  height: 35rpx;
  background:rgba(250,232,196,1);
  border-radius:4px;
  font-size:21rpx;
  line-height: 35rpx;
  text-align: center;
}

.container .PhotographerInfoWrap .infoContent .score{
  display: flex;
  align-items: center;
}

.container .PhotographerInfoWrap .infoContent .score .value{
  font-size:30rpx;
  font-weight:bold;
  color:rgba(246,32,32,1);
}

.container .PhotographerInfoWrap .infoContent .score .valueTlele{
  font-size:22rpx;
  font-weight:500;
  color:rgba(44,44,44,1);
  opacity:0.67;
  margin-left: 22rpx;
}

.container .evaluateForm .evaluateDimension{
  padding:60rpx 0;
}

.container .evaluateForm .evaluateDimension .dimensionItem{
  display: flex;
}

.container .evaluateForm .evaluateDimension .dimensionItem+.dimensionItem{
  margin-top: 60rpx;
}

.container .evaluateForm .evaluateDimension .dimensionItem .showRate{
  margin-left: auto;
}

.container .evaluateForm .OtherEvaluateWrap .title{
  font-size:36rpx;
  font-weight:bold;
  color:rgba(51,51,51,1);
}

.container .evaluateForm .OtherEvaluateWrap .OtherEvaluate textarea{
  width:660rpx;
  height:200rpx;
  background:rgba(245,246,247,1);
  border-radius:10px;
}