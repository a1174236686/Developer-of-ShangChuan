/* pages/photographer/photographer.wxss */
.main{
  color: #333;
}

.title{
  display: flex;
  align-items: center;
  padding: 0 35rpx;
}

.screen{
  flex: 1;
}

.weui-search-bar {
  position: relative;
  padding: 8px 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-sizing: border-box;
}
.weui-icon-search_in-box {
  position: absolute;
  right: 10px;
  top: 4px;
}
.weui-search-bar__form {
  position: relative;
  -webkit-box-flex: 1;
  -webkit-flex: auto;
  flex: auto;
  border-radius: 20px;
}
.weui-search-bar__box {
  position: relative;
  padding: 0 30px;
  width: 100%;
  box-sizing: border-box;
  z-index: 1;
  background-color: #F6F5F8;
  border-radius: 20px;
}
.weui-search-bar__input {
  height: 28px;
  line-height: 28px;
  font-size: 14px;
}

.bannerImg{
  width: 100%;
  height: 400rpx;
  padding: 30rpx;
}